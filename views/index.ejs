<!DOCTYPE html>
<html lang="es">

<head>
  <meta charset="UTF-8">
  <meta name="viewport"
    content="width=device-width, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=no, viewport-fit=cover">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">
  <meta name="theme-color" content="#fff">
  <link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,200,0,0" />
  <title>
    <%= title %>
  </title>

  <!-- Estilos -->
  <link rel="stylesheet" href="./public/core/css/framework7-bundle.min.css">
  <link rel="stylesheet" href="./public/core/css/fonts.css">
  <link rel="stylesheet" href="./public/core/css/colors.css?v=3">
  <link rel="stylesheet" href="./public/core/css/app.css?v=3">

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Goldman:wght@400;700&display=swap" rel="stylesheet">


  <!-- Responsive -->
  <link rel="stylesheet" type="text/css" href="./public/core/css/desktop.css" media="screen and (min-width: 768px)">
  <link rel="stylesheet" type="text/css" href="./public/core/css/mobile.css" media="screen and (max-width: 767px)">


  <!-- Iconos -->
  <link rel="apple-touch-icon" href="img/f7-icon-square.png">
  <link rel="icon" href="img/f7-icon.png">
</head>

<body>

  <div id="app">

    <div class="panel panel-left panel-floating">
      <div class="panel-content">
        <div class="login-screen-title">

          <div class="logo-creceidea" style="padding-top: 20px;">
            <img width="150px" src="public/core/images/logo_startsyncx.png" />
          </div>

        </div>
        <div class="card content-sidebar">
          <div class="card-body">
            <div class="list list-strong-ios list-outline-ios menu-list">
              <ul id="navigador-dashboard" class="row">

                <li class="medium-50 col-50">
                  <a class="item-link" href="/dashboard">

                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">dashboard</span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Dashboard</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                <li class="medium-50 col-50">
                  <a class="item-link" href="/list-category">
                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">category</span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Categorias</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                <li class="medium-50 col-50">
                  <a class="item-link" href="/dashboard">
                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">
                            menu_book
                          </span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Menu</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                <li class="medium-50 col-50">
                  <a class="item-link" href="/dashboard">
                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">
                            list_alt
                          </span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Pedidos</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                <li class="medium-50 col-50">
                  <a class="item-link" href="/dashboard">
                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">
                            draw
                          </span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Personalizar</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                <li class="medium-50 col-50">
                  <a class="item-link" href="/configuracion">
                    <div class="card card-raised card-outline">
                      <div class="card-content card-content-padding">
                        <div class="item-media">
                          <span class="material-symbols-outlined">settings</span>
                        </div>
                        <div class="item-inner">
                          <div class="item-title">Configuracion</div>
                        </div>
                      </div>
                    </div>
                  </a>
                </li>

                

              </ul>
            </div>
          </div>
        </div>
      </div>
    </div>


    <div class="view view-main view-init" data-master-detail-breakpoint="768" data-url="/">
      <%- include('header') %>
        <%- include('footer') %>
    </div>
  </div>


  <script defer src="./public/core/js/framework7-bundle.min.js"></script>
  <script defer type="module" src="./public/core/js/routes.js"></script>
  <script defer type="module" src="./public/core/js/core.js"></script>

  <script>

    console.log('<%= url_api_category %>')

  </script>


</body>

</html>